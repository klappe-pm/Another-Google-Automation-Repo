{
  "name": "workspace-automation",
  "version": "2.0.0",
  "description": "Google Apps Script automation tools with deployment status management",
  "scripts": {
    "setup": "./automation/setup/setup.sh",
    "verify": "./automation/setup/verify.sh",
    "deploy": "./automation/deployment/deploy.sh",
    "sync": "./automation/deployment/sync.sh",
    "precommit": "node automation/precommit/check-all.js",
    "fix": "node automation/fixers/fix-all.js",
    "lint": "node automation/precommit/lint-gas.js",
    "lint:fix": "node automation/precommit/lint-gas.js --fix",
    "report": "node automation/reports/project-report.js",
    "report:catalog": "node automation/reports/catalog.js",
    "repo:review": "node automation/reports/project-report.js",
    "repo:report": "node automation/reports/catalog.js",
    "version:current": "echo $npm_package_version"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/klappe-pm/another-google-automation-repo.git"
  },
  "keywords": [
    "google-apps-script",
    "automation",
    "workspace",
    "gmail",
    "calendar",
    "drive"
  ],
  "author": "Kevin Lappe <kevin.lappe@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@google/clasp": "^2.4.2",
    "googleapis": "^126.0.1"
  },
  "devDependencies": {
    "@actions/core": "^1.10.0",
    "eslint": "^8.57.0",
    "snyk": "^1.1291.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
